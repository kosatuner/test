<p id="notice"><%= notice %></p>

<h1>Listing Indicators</h1>

<table>
  <thead>
    <tr>
      <th>Valor</th>
      <th>Fecha</th>
      <th>Signal</th>
      <th>Stock</th>
      <th>Indicatortype</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @indicators.each do |indicator| %>
      <tr>
        <td><%= indicator.valor %></td>
        <td><%= indicator.fecha %></td>
        <td><%= indicator.signal %></td>
        <td><%= indicator.stock.ticker %></td>
        <td><%= indicator.indicator_type.nombre %></td>
        <td><%= link_to 'Show', indicator %></td>
        <td><%= link_to 'Edit', edit_indicator_path(indicator) %></td>
        <td><%= link_to 'Destroy', indicator, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Indicator', new_indicator_path %>
